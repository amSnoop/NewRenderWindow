<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!-- Add Resources Here -->
	<Color x:Key="Accent">#7160E8</Color>
	<Color x:Key="Main">#333</Color>
	<Color x:Key="Alt">#222</Color>
	<Color x:Key="Highlight">#555</Color>
	<Color x:Key="MoreHighlight">#AAA</Color>
	<Color x:Key="TextColor">White</Color>
	<SolidColorBrush x:Key="PatreonColor">Orange</SolidColorBrush>
	<SolidColorBrush x:Key="BackgroundMain" Color="{DynamicResource Main}"/>
	<SolidColorBrush x:Key="BackgroundMenu" Color="{DynamicResource Alt}"/>
	<SolidColorBrush x:Key="MenuButtonBackground" Color="{DynamicResource Alt}"/>
	<SolidColorBrush x:Key="MenuButtonSelectedBackground" Color="{DynamicResource Accent}" Opacity=".7"/>
	<SolidColorBrush x:Key="MenuButtonHoveredBackground" Color="{DynamicResource Highlight}" />
	<SolidColorBrush x:Key="MenuButtonSelectedHoveredBackground" Color="{DynamicResource Accent}"/>
</ResourceDictionary>
